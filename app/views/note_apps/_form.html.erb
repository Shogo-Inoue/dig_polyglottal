<%= form_with model: noteTextCard do |form| %>
  <div>
    <%= form.submit %>
  </div>

  <div>
    <%= form.label :noteText %><br>
    <%= form.text_area :noteText %>
    <% noteTextCard.errors.full_messages_for(:noteText).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

<% end %>